{
  "name": "nodeauth-challenge",
  "version": "1.0.0",
  "description": "Authentication Sprint Challenge",
  "main": "index.ts",
  "scripts": {
    "server": "nodemon index.ts",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/Sprint-Challenge-Authentication.git"
  },
  "keywords": [],
  "author": "Lambda School",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LambdaSchool/Sprint-Challenge-Authentication/issues"
  },
  "homepage": "https://github.com/LambdaSchool/Sprint-Challenge-Authentication#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.10.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.0",
    "sqlite3": "^4.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
